{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "custom",
    "plugin:@typescript-eslint/recommended",
    "plugin:unicorn/recommended"
  ],
  "plugins": ["unicorn"],
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "browser": false
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": false
    },
    "sourceType": "module"
  },
  "globals": {
    "strapi": true
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-console": 0,
    "indent": "off",
    "quotes": "off",
    "semi": "off"
  }
}
