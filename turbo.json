    "build": {
      "dependsOn": []
    },
    "deploy": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": [],
      "env": [
        "TEST_DB_HOST",
        "TEST_DB_PORT",
        "TEST_DB_USER",
        "TEST_DB_PASSWORD",
        "NATS_URL",
        "QDRANT_URL",
        "OLLAMA_BASE_URL",
        "OLLAMA_API_KEY",
        "OPENAI_API_KEY",
        "MODEL_NAME_CHAT",
